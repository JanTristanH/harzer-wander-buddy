<mvc:View controllerName="hwb.frontendhwb.controller.Map"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <VBox>
                <mvc:XMLView viewName="hwb.frontendhwb.view.MapInner"/>
            </VBox>
        </content>
        <footer>
            <core:Fragment
                fragmentName="hwb.frontendhwb.fragment.Toolbar"
                type="XML"
            />
        </footer>
    </Page>
</mvc:View>
